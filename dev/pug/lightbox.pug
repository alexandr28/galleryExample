extends templates/template

block main
    h1 LightBox Gallery
    .gallary-container#gallery-container
        - var n = 1
        while n <= 12
            .gallery-item
               img(src="img/gallery-"+ n +"-min.jpg" alt="Descripcion de la Imagen"+ n).gallery-img
            -n++
    script(src='js/lightbox.js')